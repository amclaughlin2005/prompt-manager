{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "if [ -n \"$DATABASE_URL\" ] && [[ \"$DATABASE_URL\" == postgres* ]]; then prisma generate --schema prisma/schema.postgres.prisma && prisma db push --schema prisma/schema.postgres.prisma; else prisma generate --schema prisma/schema.prisma && prisma db push --schema prisma/schema.prisma; fi && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy --schema prisma/schema.postgres.prisma"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.13.0",
    "next": "15.4.6",
    "openai": "^5.12.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.13.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
